@start
    // =====================================================
    // üîç DEMONSTRA√á√ÉO COMPLETA DA LINGUAGEM LENS
    // =====================================================
    
    // =====================================================
    // üìù DECLARA√á√ïES DE VARI√ÅVEIS (todos os tipos)
    // =====================================================
    
    // Tipos primitivos b√°sicos
    let idade: int = 25
    let altura: int = 175
    let nome: String = "Jo√£o Silva"
    
    // Declara√ß√µes sem inicializa√ß√£o
    let contador: int
    let peso: int
    let sobrenome: String
    
    // =====================================================
    // üîß ATRIBUI√á√ïES (simples e compostas)
    // =====================================================
    
    // Atribui√ß√µes simples
    contador = 0
    peso = 70
    sobrenome = "Santos"
    
    // Atribui√ß√µes compostas
    idade += 5          // idade = idade + 5 = 30
    altura -= 5      // altura = altura - 5 = 170
    contador *= 2       // contador = contador * 2 = 0
    peso /= 2           // peso = peso / 2 = 35
    
    // =====================================================
    // üì§ IMPRESS√ïES (v√°rios formatos)
    // =====================================================
    
    // Impress√£o simples
    print :: "=== DEMONSTRA√á√ÉO LENS ==="
    print :: nome
    print :: idade
    
    // Impress√£o m√∫ltipla (string + valor)
    print :: "Nome completo:", nome, sobrenome
    print :: "Idade atual:", idade
    print :: "Altura:", altura
    print :: "Peso:", peso
    
    // =====================================================
    // üî¢ EXPRESS√ïES ARITM√âTICAS
    // =====================================================
    
    let a: int = 10
    let b: int = 3
    
    let soma: int = a + b           // 13
    let subtracao: int = a - b      // 7
    let multiplicacao: int = a * b  // 30
    let divisao: int = a / b        // 3
    
    print :: "Opera√ß√µes com", a, "e", b
    print :: "Soma:", soma
    print :: "Subtra√ß√£o:", subtracao
    print :: "Multiplica√ß√£o:", multiplicacao
    print :: "Divis√£o:", divisao
    
    // Express√µes complexas com preced√™ncia
    let resultado: int = a + b * 2 - 1    // 10 + 6 - 1 = 15
    let complexo: int = (a + b) * 2       // (10 + 3) * 2 = 26
    
    print :: "Resultado complexo:", resultado
    print :: "Com par√™nteses:", complexo
    
    // =====================================================
    // üîç EXPRESS√ïES DE COMPARA√á√ÉO
    // =====================================================
    
    let igual: int = a == b        // 0 (falso)
    let diferente: int = a != b    // 1 (verdadeiro)
    let menor: int = a < b         // 0
    let maior: int = a > b         // 1
    let menorIgual: int = a <= b   // 0
    let maiorIgual: int = a >= b   // 1

    print :: "=== COMPARA√á√ïES ==="
    print :: "10 == 3:", igual
    print :: "10 != 3:", diferente
    print :: "10 < 3:", menor
    print :: "10 > 3:", maior
    print :: "10 <= 3:", menorIgual
    print :: "10 >= 3:", maiorIgual
    
    // =====================================================
    // üß† EXPRESS√ïES L√ìGICAS
    // =====================================================
    
    let condicao1: int = 1      // 1 para verdadeiro
    let condicao2: int = 0      // 0 para falso

    let e_logico: int = condicao1 && condicao2     // 0
    let ou_logico: int = condicao1 || condicao2    // 1

    print :: "=== OPERA√á√ïES L√ìGICAS ==="
    print :: "1 && 0:", e_logico
    print :: "1 || 0:", ou_logico

    // Express√£o l√≥gica complexa
    let complexa: int = (a > b) && (soma > 10)     // 1 && 1 = 1
    print :: "Express√£o complexa:", complexa
    
    // =====================================================
    // üì• ENTRADA DE DADOS
    // =====================================================
    
    let entrada_usuario: int
    print :: "Digite um n√∫mero:"
    input(entrada_usuario)
    
    print :: "Voc√™ digitou:", entrada_usuario
    
    // =====================================================
    // üîÄ ESTRUTURAS CONDICIONAIS (if/elseif/else)
    // =====================================================
    
    print :: "=== ESTRUTURAS CONDICIONAIS ==="
    
    // Condicional simples
    if (idade >= 18) {
        print :: "Maior de idade"
    }
    
    // Condicional completa
    if (idade < 13) {
        print :: "Crian√ßa"
    } elseif (idade < 18) {
        print :: "Adolescente"
    } elseif (idade < 60) {
        print :: "Adulto"
    } else {
        print :: "Idoso"
    }
    
    // =====================================================
    // üîÅ LA√áOS FOR (itera√ß√£o com range)
    // =====================================================
    
    print :: "=== LA√áOS FOR ==="
    
    // For b√°sico
    print :: "Contagem de 1 a 5:"
    for i in 1..5 {
        print :: "N√∫mero:", i
    }
    
    // For com range negativo
    print :: "Contagem regressiva:"
    for j in 10..7 {
        print :: "Contagem:", j
    }
    
    // For com vari√°veis
    let inicio: int = 0
    let fim: int = 3
    print :: "Range din√¢mico:"
    for k in inicio..fim {
        print :: "Valor de k:", k
        let quadrado: int = k * k
        print :: "Quadrado:", quadrado
    }
    
    // =====================================================
    // üîÑ LA√áOS WHILE (itera√ß√£o condicional)
    // =====================================================
    
    print :: "=== LA√áOS WHILE ==="
    
    // While b√°sico
    let cont: int = 0
    print :: "Contagem while at√© 3:"
    while (cont < 3) {
        print :: "Contador:", cont
        cont += 1
    }
    
    // While com condi√ß√£o complexa
    let x: int = 10
    let y: int = 1
    print :: "While com condi√ß√£o complexa:"
    while (x > y && y < 5) {
        print :: "x =", x, "y =", y
        x -= 2
        y += 1
    }
    
    // =====================================================
    // üéØ CASOS DE USO AVAN√áADOS
    // =====================================================
    
    print :: "=== CASOS AVAN√áADOS ==="
    
    // Calculadora simples
    let num1: int = 15
    let num2: int = 4
    let operacao: int = 1  // 1=soma, 2=sub, 3=mul, 4=div
    
    if (operacao == 1) {
        let result: int = num1 + num2
        print :: "Soma:", num1, "+", num2, "=", result
    } elseif (operacao == 2) {
        let result: int = num1 - num2
        print :: "Subtra√ß√£o:", num1, "-", num2, "=", result
    } elseif (operacao == 3) {
        let result: int = num1 * num2
        print :: "Multiplica√ß√£o:", num1, "*", num2, "=", result
    } elseif (operacao == 4) {
        if (num2 != 0) {
            let result: int = num1 / num2
            print :: "Divis√£o:", num1, "/", num2, "=", result
        } else {
            print :: "Erro: Divis√£o por zero!"
        }
    }
    
    // Fatorial simples (usando for)
    let n: int = 5
    let fatorial: int = 1
    print :: "Calculando fatorial de", n
    
    for f in 1..n {
        fatorial *= f
        print :: "Passo", f, ":", fatorial
    }
    print :: "Fatorial final:", fatorial
    
    // Sequ√™ncia de Fibonacci (primeiros 10 n√∫meros)
    let fib1: int = 0
    let fib2: int = 1
    let fib_count: int = 0
    
    print :: "Sequ√™ncia de Fibonacci:"
    print :: "F(0) =", fib1
    print :: "F(1) =", fib2
    
    while (fib_count < 8) {
        let proximo: int = fib1 + fib2
        fib_count += 1
        print :: "F(" , fib_count + 1, ") =", proximo
        fib1 = fib2
        fib2 = proximo
    }
    
    // Valida√ß√£o de dados
    let nota: int = 8
    let frequencia: int = 85
    
    print :: "=== VALIDA√á√ÉO DE APROVA√á√ÉO ==="
    print :: "Nota:", nota
    print :: "Frequ√™ncia:", frequencia
    
    if (nota >= 7 && frequencia >= 75) {
        print :: "APROVADO! ‚úì"
    } elseif (nota >= 5 && frequencia >= 75) {
        print :: "RECUPERA√á√ÉO"
    } else {
        print :: "REPROVADO ‚úó"
    }
    
    // =====================================================
    // üéâ FINALIZA√á√ÉO
    // =====================================================
    
    print :: "=========================="
    print :: "Demonstra√ß√£o finalizada!"
    print :: "Todas as funcionalidades foram testadas:"
    print :: "‚úì Tipos: int, String"
    print :: "‚úì Operadores: +, -, *, /, ==, !=, <, >, <=, >=, &&, ||"
    print :: "‚úì Estruturas: if/elseif/else, for, while"
    print :: "‚úì E/S: print, input"
    print :: "‚úì Express√µes complexas e preced√™ncia"
    print :: "=========================="

@end